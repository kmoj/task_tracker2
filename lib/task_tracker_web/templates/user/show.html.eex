<div class="container">
    <h2>Show User</h2>

    <ul>

      <li>
        <strong>Name:</strong>
        <%= @user.name %>
      </li>

      <li>
        <strong>Email:</strong>
        <%= @user.email %>
      </li>

      <li>
        <strong>Manage level:</strong>
        <%= @user.manage_level %>
      </li>

    </ul>

    <span><%= link "Back", to: page_path(@conn, :index), class: "btn btn-danger" %></span>
</div>
